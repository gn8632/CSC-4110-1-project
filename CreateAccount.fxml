<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CreateAccount">
    <children>
        <TextField fx:id="usercreate" layoutX="44.0" layoutY="97.0" promptText="UserName">
            <font>
                <Font size="18.0" />
            </font>
        </TextField>
        <PasswordField fx:id="passwordcreate" layoutX="44.0" layoutY="169.0" promptText="Password">
            <font>
                <Font size="18.0" />
            </font>
        </PasswordField>
        <Label fx:id="loginfx1" layoutX="79.0" layoutY="28.0" prefHeight="39.0" prefWidth="206.0" text="Create Account" textFill="GREEN">
            <font>
                <Font name="System Bold Italic" size="18.0" />
            </font>
        </Label>
        <Button layoutX="52.0" layoutY="245.0" mnemonicParsing="false" onAction="#createaccountnow" text="Create Account" />
        <Button fx:id="Close" cancelButton="true" layoutX="206.0" layoutY="245.0" mnemonicParsing="false" onAction="#finish" text="Close" />
        <Label fx:id="success" layoutX="45.0" layoutY="213.0" prefHeight="27.0" prefWidth="245.0" textFill="Blue">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
