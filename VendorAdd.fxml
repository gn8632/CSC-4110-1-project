<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="472.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

    <children>
        <Label alignment="CENTER" prefHeight="38.0" prefWidth="450.0" text="Vendor Log">
            <font>
                <Font size="38.0" />
            </font>
        </Label>
        <Button fx:id="Close" cancelButton="true" layoutX="196.0" layoutY="405.0" mnemonicParsing="false" onAction="#finish" prefHeight="38.0" prefWidth="117.0" text="Close" />
        <TextField fx:id="vendorName" layoutX="-3.0" layoutY="107.0" prefHeight="26.0" prefWidth="457.0" promptText="Vendor Name" />
        <TextField fx:id="managerName" layoutX="-3.0" layoutY="133.0" prefHeight="26.0" prefWidth="457.0" promptText="Manager Name" />
        <TextField fx:id="companyEmail" layoutX="1.0" layoutY="185.0" prefHeight="26.0" prefWidth="450.0" promptText="Company Email" />
        <TextField fx:id="companyNumber" layoutX="-3.0" layoutY="159.0" prefHeight="26.0" prefWidth="457.0" promptText="Company Number" />
        <TextField fx:id="companyAddress" layoutY="211.0" prefHeight="32.0" prefWidth="450.0" promptText="Company Address" />
        <TextField fx:id="taxID" layoutX="-3.0" layoutY="243.0" prefHeight="32.0" prefWidth="457.0" promptText="Tax ID" />
        <Button fx:id="createAccount" layoutX="14.0" layoutY="283.0" mnemonicParsing="false" onAction="#addData" text="Create Account" />
        <TextField fx:id="inputVendor" layoutX="14.0" layoutY="351.0" onAction="#deleteVendor" prefHeight="26.0" prefWidth="300.0" promptText="Enter Vendor Name:" />
        <Button fx:id="deleteAccount" alignment="CENTER" layoutX="14.0" layoutY="405.0" mnemonicParsing="false" onAction="#deleteVendor" prefHeight="38.0" prefWidth="117.0" text="Delete Account" textAlignment="CENTER" />
    </children>
</AnchorPane>
